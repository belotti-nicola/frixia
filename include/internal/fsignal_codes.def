X(FSIGNAL_TERMINATE,(1 << 2)) // 4   -> Mappa a SIGHUP
X(FSIGNAL_USR1,(1 << 3)) // 8   -> Mappa a SIGUSR1
X(FSIGNAL_USR2,(1 << 4)) // 16  -> Mappa a SIGUSR2
X(FSIGNAL_TERM,(1 << 5)) // 32  -> Mappa a SIGTERM
X(FSIGNAL_QUIT,(1 << 6)) // 64  -> Mappa a SIGQUIT
X(FSIGNAL_ABRT,(1 << 7)) // 128 -> Mappa a SIGABRT
X(FSIGNAL_ALRM,(1 << 8)) // 256 -> Mappa a SIGALRM
X(FSIGNAL_CHILD,(1 << 9)) // 512 -> Mappa a SIGCHLD
X(FSIGNAL_PIPE,(1 << 10))// 1024 -> Mappa a SIGPIPE
X(FSIGNAL_STOP,(1 << 11))// 2048 -> Mappa a SIGSTOP
X(FSIGNAL_TSTP,(1 << 12))// 4096 -> Mappa a SIGTSTP
X(FSIGNAL_CONT,(1 << 13))// 8192 -> Mappa a SIGCONT
X(FSIGNAL_SEGV,(1 << 14))// 16384 -> Mappa a SIGSEGV
X(FSIGNAL_BUS,(1 << 15))// 32768 -> Mappa a SIGBUS
X(FSIGNAL_FPE,(1 << 16))// 65536 -> Mappa a SIGFPE
X(FSIGNAL_ILL,(1 << 17))// 131072 -> Mappa a SIGILL
X(FSIGNAL_SYS,(1 << 18))// 262144 -> Mappa a SIGSYS
X(FSIGNAL_TRAP,(1 << 19))// 524288 -> Mappa a SIGTRAP
X(FSIGNAL_PWR,(1 << 20))// 1048576 -> Mappa a SIGPWR
X(FSIGNAL_WINCH,(1 << 21))// 2097152 -> Mappa a SIGWINCH
  
X(FSIGNAL_ALL,0xFFFFFFFF)// Maschera per tutti i segnali

